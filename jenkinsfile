pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the file'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'i have created my first pipeline'
            }
        }
    }
}
